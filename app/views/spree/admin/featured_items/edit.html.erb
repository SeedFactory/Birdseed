<% content_for :page_title do %>
  <%= Spree.t(:editing_resource, resource: Spree::FeaturedItem.model_name.human) %>
<% end %>

<%= form_for @featured_item, url: object_url, method: :put do |f| %>
  <div class="row">
    <div class="col-md-12">
      <%= render 'form', f: f %>
      <%= render 'spree/admin/shared/edit_resource_links' %>  
    </div>    
  </div>
<% end %>
