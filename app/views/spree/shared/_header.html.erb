<header class="birdseed-header container-fluid">
  <h1 class="birdseed-logo">
    <%= link_to :root do %>
      <%= image_tag 'logo-white.png', class: 'birdseed-logo-white' %>
      <%= image_tag 'logo-black.png', class: 'birdseed-logo-black' %>
    <% end %>
  </h1>
  <nav class="birdseed-nav">
    <%= link_to 'Shop', Rails.application.routes.url_helpers.shop_path %>
    <%#= link_to 'FAQ', Rails.application.routes.url_helpers.faq_path %>
    <%= link_to 'About', Rails.application.routes.url_helpers.about_path %>
    <%= link_to 'Talk To Us', Rails.application.routes.url_helpers.talk_to_us_path %>
    <% if spree_current_user %>
      <%= link_to 'Logout', logout_path %>
    <% else %>
      <%= link_to 'Login', login_path %>
    <% end %>
  </nav>
  <a href="" class="birdseed-nav-toggle">
    <span class="birdseed-nav-toggle-icon-bar"></span>
    <span class="birdseed-nav-toggle-icon-bar"></span>
  </a>
</header>
